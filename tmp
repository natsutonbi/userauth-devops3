CREATE TABLE user_permissions (
  username VARCHAR(32) references user_login(username),
  path VARCHAR(200) references permissions(path),
  methods VARCHAR(100) references permissions(methods),
  outdate_time TIMESTAMP NOT NULL,
  create_user VARCHAR(32) NOT NULL,
  created_time TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  update_user VARCHAR(32) NOT NULL,
  updated_time TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (username,path,methods)
);
ALTER TABLE `user_permissions` add constraint fky foreign key (username) references user_login(username);